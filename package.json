{
  "name": "joinsoraa",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:stamp": "NEXT_PUBLIC_COMMIT_SHA=$(npm run -s print:commit) NEXT_PUBLIC_BUILD_TIME=$(date -u +%Y-%m-%dT%H:%M:%SZ) next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "print:commit": "node -e \"console.log((process.env.VERCEL_GIT_COMMIT_SHA||'') || require('child_process').execSync('git rev-parse HEAD').toString().trim())\""
  },
  "dependencies": {
    "next": "14.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "framer-motion": "^10.16.4",
    "clsx": "^2.0.0",
    "tailwind-merge": "^2.0.0",
    "lucide-react": "^0.378.0"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "postcss": "^8",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  }
}
